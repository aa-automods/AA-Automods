{
    "hosting": {
        "public": ".",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }],
        "redirects": [{
            "source": "/www.aa-automods.ca",
            "destination": "https://aa-automods.ca",
            "type": 301

        }]
    }
}